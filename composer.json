{
  "name": "nolex/slim-backend-template",
  "description": "A robust, scalable, and secure Slim backend template with Eloquent ORM",
  "license": "MIT",
  "type": "project",
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": "phpunit",
    "phinx-create-migration": "phinx create",
    "phinx-migrate": "phinx migrate",
    "phinx-create-seed": "phinx seed:create",
    "phinx-status": "phinx status",
    "phinx-seed": "phinx seed:run",
    "phinx-rollback": "phinx rollback"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "App\\Models\\": "src/models/",
      "App\\Controllers\\": "src/controllers/",
      "App\\Services\\": "src/services/",
      "App\\Helper\\": "src/helpers/",
      "App\\Middleware\\": "src/middleware/",
      "App\\Routes\\": "src/routes/"
    }
  },
  "require": {
    "firebase/php-jwt": "^6.11",
    "illuminate/container": "^10.49",
    "illuminate/database": "^10.0",
    "illuminate/events": "^10.49",
    "monolog/monolog": "^3.5",
    "php-di/php-di": "^7.0",
    "phpmailer/phpmailer": "^6.10",
    "ramsey/uuid": "^4.8",
    "respect/validation": "^2.4",
    "robmorgan/phinx": "^0.15",
    "slim/psr7": "^1.6",
    "slim/slim": "^4.12",
    "vlucas/phpdotenv": "^5.6"
  },
  "authors": [
    {
      "name": "Gilbert Elikplim Kukah",
      "email": "kwamegilbert1114@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true,
    "process-timeout": 0
  },
  "minimum-stability": "stable"
}
